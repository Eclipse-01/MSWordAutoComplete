{
  "plugins": [
    "office-addins"
  ],
  "extends": [
    "plugin:office-addins/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "console": "readonly",
    "window": "readonly",
    "localStorage": "readonly",
    "process": "readonly"
  },
  "rules": {
    "no-undef": "off"
  }
}
